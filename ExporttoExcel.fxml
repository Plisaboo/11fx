<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="task11.ExportToExcelController"
            prefWidth="350" prefHeight="220">
    <VBox spacing="10" alignment="CENTER" layoutX="50" layoutY="20">
        <Label text="Экспорт в Excel" style="-fx-font-size:16px;"/>
        <ComboBox fx:id="tableCombo" promptText="Выберите таблицу"/>
        <TextField fx:id="fileNameField" promptText="Имя Excel-файла без расширения"/>
        <Button text="Экспортировать" onAction="#onExport"/>
        <Button text="Назад" onAction="#onBack"/>
    </VBox>
</AnchorPane>